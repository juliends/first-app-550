<h1>Contact page</h1>


<form action="/contact" method="get">
  <input type="text" name="member" placeholder="Who are you looking for?">
  <input type="submit">
</form>

<ul>
  <% @members.each do |member| %>
    <li><%= member %></li>
  <% end %>
</ul>

<%= link_to "About page", about_path, class: 'btn btn-primary' %>


